else
{
  var prevValue = state.get("10");
  
  if( pin10 != prevValue ) 
  {
     if( pin10 <= 127 && prevValue > 127 )
     {
        setTone( pin10);
     }
     else if( pin10 > 127 && prevValue <=127 )
     {
        setTone(pin10);       
     }
     
     state.put("10", new java.lang.Integer( pin10) );
  }
}

